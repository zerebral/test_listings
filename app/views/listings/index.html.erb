<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Heading</th>
      <th>Vin</th>
      <th>Price</th>
      <th>Miles</th>
      <th>Decimal</th>
      <th>Msrp</th>
      <th>Vdp url</th>
      <th>Stock no</th>
      <th>Exterior color</th>
      <th>Interior color</th>
      <th>Seller comments</th>
      <th>Details</th>
      <th>Seller name</th>
      <th>Seller phone</th>
      <th>City state zip</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Photo link</th>
      <th>Photo links</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.heading %></td>
        <td><%= listing.vin %></td>
        <td><%= listing.price %></td>
        <td><%= listing.miles %></td>
        <td><%= listing.decimal %></td>
        <td><%= listing.msrp %></td>
        <td><%= listing.vdp_url %></td>
        <td><%= listing.stock_no %></td>
        <td><%= listing.exterior_color %></td>
        <td><%= listing.interior_color %></td>
        <td><%= listing.seller_comments %></td>
        <td><%= listing.details %></td>
        <td><%= listing.seller_name %></td>
        <td><%= listing.seller_phone %></td>
        <td><%= listing.city_state_zip %></td>
        <td><%= listing.city %></td>
        <td><%= listing.state %></td>
        <td><%= listing.zip %></td>
        <td><%= listing.photo_link %></td>
        <td><%= listing.photo_links %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
